{
  "manifest_version": 3,
  "name": "DocGen Extension",
  "version": "1.0",
  "description": "Extension for generative documentation.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "assets/icon.png"
  },
  "icons": {
    "128": "assets/icon.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ]
}